<h1>ログイン</h1>
  <%= form_for :session, url: login_path do |f| %>
  <%= f.label :メールアドレス %>
  <%= f.text_field :email %>
  <%= f.label :パスワード %>
  <%= f.password_field :password, class: "form-control" %>
  <%= f.submit "送信", class: "btn-block btn-white" %>
<% end %>
<%= link_to '新規登録はこちら', new_user_path %>